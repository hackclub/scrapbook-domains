{
  "public": true,
  "github": { "silent": true },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/(.+)",
      "destination": "https://scrapbook.hackclub.com/$1"
    }
  ],
  "redirects": [
    {
      "source": "/next/image",
      "destination": "https://scrapbook.hackclub.com/next/image"
    }
  ]
}
